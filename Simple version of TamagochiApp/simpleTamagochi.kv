#:kivy 1.0.9

<TamagochiProject>:
    feedButton: feed_button.__self__

    size: 375, 812

    canvas:
        Rectangle:
            size: root.size
            pos: root.pos
            source: "../images/tamagochi.jpg"
    Label:
        font_size: 25
        center_x: root.center_x
        top: root.top
        text: "This is your Tamagochi"
        background_color: 193/255, 200/255, 199/255, 1
    Button:
        id: feed_button
        background_color: 94/256, 102/256, 104/256
        text: "Feed Tamagochi"
        x: root.x
        y: root.y
        size: root.width, 50


<ProjectPage>:
    addProjectButton: add_roject_button.__self__

    Button:
        id: add_roject_button
        background_color: 94/256, 102/256, 104/256
        text: "Add project"
        x: root.x
        top: root.top
        size: root.width, 50

